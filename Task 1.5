        Dim membername As String
        Dim id As Integer
        Dim telNum As String
        Dim startDate As Date

        FileOpen(1, "D:\members.txt", OpenMode.Input)
        FileOpen(2, "D:\temp.txt", OpenMode.Output)
        Do
            Input(1, membername)
            Input(1, id)
            Console.Write("Enter telephone number for " & membername & ": ")
            telNum = Console.ReadLine()
            Console.Write("Enter membership start date for " & membername & ": ")
            startDate = Console.ReadLine()
            WriteLine(2, membername)
            WriteLine(2, id)
            WriteLine(2, telNum)
            WriteLine(2, startDate)
        Loop Until EOF(1)
        FileClose(1)
        FileClose(2)
        My.Computer.FileSystem.DeleteFile("D:\members.txt")
        My.Computer.FileSystem.RenameFile("D:\temp.txt", "members.txt")
        Console.ReadKey()
